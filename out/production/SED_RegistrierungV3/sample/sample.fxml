<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.2" fx:controller="sample.Controller">

    <VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
        <children>
            <MenuBar VBox.vgrow="NEVER">
                <menus>
                    <Menu mnemonicParsing="false" text="File">
                        <items>
                            <MenuItem mnemonicParsing="false" text="New" />
                            <MenuItem mnemonicParsing="false" text="Open…" />
                            <Menu mnemonicParsing="false" text="Open Recent" />
                            <SeparatorMenuItem mnemonicParsing="false" />
                            <MenuItem mnemonicParsing="false" text="Close" />
                            <MenuItem mnemonicParsing="false" text="Save" />
                            <MenuItem mnemonicParsing="false" text="Save As…" />
                            <MenuItem mnemonicParsing="false" text="Revert" />
                            <SeparatorMenuItem mnemonicParsing="false" />
                            <MenuItem mnemonicParsing="false" text="Preferences…" />
                            <SeparatorMenuItem mnemonicParsing="false" />
                            <MenuItem mnemonicParsing="false" text="Quit" />
                        </items>
                    </Menu>
                    <Menu mnemonicParsing="false" text="Edit">
                        <items>
                            <MenuItem mnemonicParsing="false" text="Undo" />
                            <MenuItem mnemonicParsing="false" text="Redo" />
                            <SeparatorMenuItem mnemonicParsing="false" />
                            <MenuItem mnemonicParsing="false" text="Cut" />
                            <MenuItem mnemonicParsing="false" text="Copy" />
                            <MenuItem mnemonicParsing="false" text="Paste" />
                            <MenuItem mnemonicParsing="false" text="Delete" />
                            <SeparatorMenuItem mnemonicParsing="false" />
                            <MenuItem mnemonicParsing="false" text="Select All" />
                            <MenuItem mnemonicParsing="false" text="Unselect All" />
                        </items>
                    </Menu>
                    <Menu mnemonicParsing="false" text="Help">
                        <items>
                            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
                        </items>
                    </Menu>
                </menus>
            </MenuBar>
            <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="415.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
                <children>
                    <Label alignment="CENTER" layoutX="22.0" layoutY="161.0" style="&#10;" text="Drag components from Library here…" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
                        <font>
                            <Font size="18.0" />
                        </font>
                    </Label>
                    <Button layoutX="22.0" layoutY="318.0" mnemonicParsing="false" onAction="#formAuswertung" prefHeight="25.0" prefWidth="122.0" style="-fx-background-color: blue;" text="Anmelden" textFill="WHITE" />
                    <TextField fx:id="vorname" accessibleText="button1" layoutX="22.0" layoutY="49.0" promptText="Vorname" />
                    <TextField fx:id="nachname" layoutX="191.0" layoutY="49.0" promptText="Nachname" />
                    <RadioButton fx:id="lehrender" layoutX="22.0" layoutY="22.0" mnemonicParsing="false" text="Lehrender">
                        <toggleGroup>
                            <ToggleGroup fx:id="rolle" />
                        </toggleGroup>
                    </RadioButton>
                    <RadioButton fx:id="studierender" layoutX="128.0" layoutY="22.0" mnemonicParsing="false" text="Studierender" toggleGroup="$rolle" />
                    <TextField fx:id="email" layoutX="22.0" layoutY="162.0" promptText="Email" />
                    <TextField fx:id="straße" layoutX="22.0" layoutY="89.0" promptText="Straße" />
                    <TextField fx:id="hausnummer" layoutX="191.0" layoutY="89.0" promptText="Hausnummer" />
                    <TextField fx:id="lehrstuhlfeld" layoutX="22.0" layoutY="127.0" promptText="Lehrstuhl" />
                    <PasswordField fx:id="password" layoutX="22.0" layoutY="201.0" promptText="Password" />
                    <PasswordField fx:id="password_bes" layoutX="22.0" layoutY="237.0" promptText="Password bestätigen" />
                </children>
            </AnchorPane>
        </children>
    </VBox>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
